@page
@model SejlBåd.Pages.DockSpotPages.RentDockSpotModel
@{
}

<h2>Rent Dock Spot</h2>

<form method="Post">

    <div class="form-group">
        <label asp-for="Renter.FirstName" class="control-label"></label>
        <input asp-for="Renter.FirstName" class="form-control" />
        <span asp-validation-for="Renter.FirstName" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Renter.LastName" class="control-label"></label>
        <input asp-for="Renter.LastName" class="form-control" />
        <span asp-validation-for="Renter.LastName" class="text-danger"></span>
    </div>
        <div class="form-group">
        <label asp-for="Renter.Email" class="control-label"></label>
        <input asp-for="Renter.Email" class="form-control" />
        <span asp-validation-for="Renter.Email" class="text-danger"></span>
    </div>
        <div class="form-group">
        <label asp-for="Renter.Tlf" class="control-label"></label>
        <input asp-for="Renter.Tlf" class="form-control" />
        <span asp-validation-for="Renter.Tlf" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Renter.CreditCardInfo" class="control-label"></label>
        <input asp-for="Renter.CreditCardInfo" class="form-control" />
        <span asp-validation-for="Renter.CreditCardInfo" class="text-danger"></span>
    </div>

    <p>
        <div class="form-control">
            <input type="submit" value="Rent" class="btn btn-primary" />
        </div>
    </p>

</form>
<p>
    <a asp-page="/ViewDockSpot">Return</a>
</p>