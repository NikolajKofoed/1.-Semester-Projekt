@page
@model SejlBåd.Pages.Account.Register.EmailAndPhoneNumModel
@{
}

<div class="account-register-form">
    <br />

    <form method="post">
        <br />
        <div class="account-register-form-input-label">
            <div>
                <label asp-for="Email">Email:</label>
                <input asp-for="Email" required />
            </div>
            <div>
                <label asp-for="PhoneNum">PhoneNum:</label>
                <input asp-for="PhoneNum" required />
            </div>
        </div>


        <button type="submit">Bekræft</button>
    </form>
</div>