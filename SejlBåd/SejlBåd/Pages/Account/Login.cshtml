@page
@model SejlBåd.Pages.Account.LoginModel
@{
}




<center><h2>loginpage</h2></center>
<div class="account-register-form">
    <br />


    <form method="post">
        <br />
        <div class="account-register-form-input-label">
            <div>
                <label asp-for="UserName">Username:</label>
                <input asp-for="UserName" required />
            </div>
            <div>
                <label asp-for="Password">Password:</label>
                <input asp-for="Password" required />
            </div>
        </div>

        <p>
            <a type="button" asp-page="Register/DateAndCountry">Register</a>
        </p>
        <button type="submit">Login</button>
    </form>

</div>